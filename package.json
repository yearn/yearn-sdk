{
  "name": "@yfi/sdk",
  "version": "0.0.18",
  "description": "Interaction framework for the yearn protocol",
  "author": "nymmrx <nymmrx@protonmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/core.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint src test",
    "docs": "typedoc --out docs src/index.ts",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "release": "release-it"
  },
  "peerDependencies": {
    "@ethersproject/address": "^5.1.0",
    "@ethersproject/bignumber": "^5.1.0",
    "@ethersproject/contracts": "^5.1.0",
    "@ethersproject/providers": "^5.1.0"
  },
  "devDependencies": {
    "@ethersproject/address": "^5.1.0",
    "@ethersproject/bignumber": "^5.1.0",
    "@ethersproject/contracts": "^5.1.0",
    "@ethersproject/providers": "^5.1.0",
    "@size-limit/preset-small-lib": "^4.10.1",
    "@solidity-parser/parser": "^0.12.0",
    "@types/object-hash": "^2.1.0",
    "release-it": "^14.6.1",
    "size-limit": "^4.10.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typedoc": "^0.20.33"
  },
  "dependencies": {
    "cross-fetch": "^3.1.3-alpha.6",
    "events": "^3.3.0",
    "object-hash": "^2.1.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "size-limit": [
    {
      "path": "dist/sdk.cjs.production.min.js",
      "limit": "25 KB"
    },
    {
      "path": "dist/sdk.esm.js",
      "limit": "25 KB"
    }
  ],
  "prettier": {
    "endOfLine": "auto"
  },
  "keywords": [
    "yearn",
    "crypto",
    "sdk",
    "typescript"
  ]
}
